<h1>Entrar</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <%= form_for(:session, url: entrar_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :senha %>
    <%= f.password_field :password %>

    <%= f.submit "Entrar", class: "btn btn-primary" %>
  <% end %>
</div>
</div>
